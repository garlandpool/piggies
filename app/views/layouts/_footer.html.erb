<footer class="copyright">
<div class="container">
    <div class="row">
        <div class="col-sm-4">
            <span class="copyright-header">&copy; 2015 piggies+paws :: All rights reserved
            <% if admin_signed_in? %>
                | <%= current_admin.email %> | <%= current_admin.id %>
            <% elsif user_signed_in? %>
                | <%= current_user.email %> | <%= current_user.id %>
            <% else %>
                | Not logged in
            <% end %>
            </span>
            
        </div>
        <div class="col-sm-4">
            <ul class="list-inline social-buttons">
                <li><a class="twitter wow animated bounceInDown animated animation-delay-3" href="https://twitter.com/piggiesandpaws/status/665643204843208704" target="_blank"><i class="fa fa-twitter"></i></a>
                </li>
                <li><a class="facebook wow animated bounceInDown animated animation-delay-4" href="https://www.facebook.com/PiggiesandPaws/?fref=ts&ref=br_tf" target="_blank"><i class="fa fa-facebook"></i></a>
                </li>
                <li><a class="linkedin wow animated bounceInDown animated animation-delay-5" href="https://www.linkedin.com/company/piggies-&-paws" target="_blank"><i class="fa fa-linkedin"></i></a>
                </li>
            </ul>
        </div>
        <div class="col-sm-4">
            <ul class="list-inline quicklinks">
                <li><a href="#">Privacy Policy</a></li> |
                <li><a href="#">Terms of Use</a></li> |
                <% if admin_signed_in? %>
                    <%= link_to "Logout", destroy_admin_session_path, method: :delete %>
                <% else %>
                    <%= link_to "Admin", {:controller => '/admins', :action => 'sign_in'} %>
                <% end %>
            </ul>
        </div>
    </div>
</div>
</footer>

